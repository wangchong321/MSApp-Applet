/* pages/sign-up/warning/warings.wxss */
.bt-container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.bt-normal {
  width: 90%;
  color: white !important;
  background: skyblue !important;
  height: 120rpx;
  line-height: 120rpx;
  margin-top: 10rpx;
}

.modal-over-limit {
  height: 120rpx;
  text-align: center;
  padding: 10rpx 20rpx 10rpx 20rpx;
}

.title-text {
  color: black;
  font-size: 28rpx;
}

.protocol-text {
  color: skyblue;
  font-size: 28rpx;
  text-decoration:underline skyblue;
}

/**/
.modal-receive-tip {
  height: 180rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
  padding: 10rpx 30rpx 10rpx 30rpx;
}

.sub-title-text {
  color: rgb(155, 150, 150);
  font-size: 24rpx;
  margin-top: 10rpx;
}

/**/
.modal-fail-tip {
  height: 180rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
  padding: 10rpx 30rpx 10rpx 30rpx;
}